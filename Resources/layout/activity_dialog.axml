<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent">
    <!--Single-->
    <RelativeLayout
        android:id="@+id/SignalView"
        android:layout_width="343dp"
        android:layout_height="157dp"
        android:visibility="gone"
        android:layout_centerInParent="true"
        android:background="@drawable/dialog_signal">
        <TextView
            android:id="@+id/tv_close"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginTop="4dp"
            android:layout_marginRight="11dp"
            android:layout_alignParentRight="true"/>
        <EditText
            android:id="@+id/et_signal"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_marginLeft="98dp"
            android:layout_marginTop="57dp"
            android:lines="1"
            android:background="@android:color/transparent"/>
        <TextView
            android:id="@+id/tv_cancel"
            android:layout_width="79dp"
            android:layout_height="33dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="150dp"/>
        <TextView
            android:id="@+id/tv_confirm"
            android:layout_width="79dp"
            android:layout_height="33dp"
            android:layout_alignBottom="@id/tv_cancel"
            android:layout_marginLeft="230dp"/>
    </RelativeLayout>

    <!--variable init-->
    <RelativeLayout
        android:id="@+id/VariableIintView"
        android:layout_width="333dp"
        android:layout_height="199dp"
        android:visibility="gone"
        android:background="@drawable/dialog_variable_init"
        android:layout_centerInParent="true">
        <TextView
            android:id="@+id/tv_close"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginTop="4dp"
            android:layout_marginRight="11dp"
            android:layout_alignParentRight="true"/>
        <EditText
            android:id="@+id/et_name"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:layout_marginLeft="136dp"
            android:layout_marginTop="53dp"
            android:lines="1"
            android:background="@android:color/transparent"/>
        <EditText
            android:id="@+id/et_value"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:layout_marginLeft="136dp"
            android:layout_marginTop="102dp"
            android:lines="1"
            android:background="@android:color/transparent"/>
        <TextView
            android:id="@+id/tv_cancel"
            android:layout_width="79dp"
            android:layout_height="33dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="18dp"
            android:layout_marginLeft="138dp"/>
        <TextView
            android:id="@+id/tv_confirm"
            android:layout_width="79dp"
            android:layout_height="33dp"
            android:layout_alignBottom="@id/tv_cancel"
            android:layout_marginLeft="216dp"/>
    </RelativeLayout>

    
    <!--variable increase or set-->
    <RelativeLayout
