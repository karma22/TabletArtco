    <!--image select-->
    <RelativeLayout
        android:id="@+id/ImageSelectView"
        android:layout_width="270dp"
        android:layout_height="339dp"
        android:visibility="gone"
        android:background="@drawable/dialog_collision_bg"
        android:layout_centerInParent="true">
